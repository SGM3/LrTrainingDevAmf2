<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 6.2.0//EN"
"http://www.liferay.com/dtd/liferay-resource-action- mapping_6_2_0.dtd">

<resource-action-mapping>
	<portlet-resource>
		<portlet-name>amf-event-monitor</portlet-name>
        <permissions>
            <supports>
                <action-key>VIEW_ALL_ENTRIES</action-key>
                <action-key>VIEW_SELF_ENTRIES</action-key>
				<action-key>VIEW</action-key>
            </supports>
            <site-member-defaults>
                <action-key>VIEW_SELF_ENTRIES</action-key>
                <action-key>VIEW</action-key>
            </site-member-defaults>
            <guest-defaults />
            <guest-unsupported>
                <action-key>VIEW_SELF_ENTRIES</action-key>
                <action-key>VIEW_ALL_ENTRIES</action-key>
				<action-key>VIEW</action-key>
            </guest-unsupported>
        </permissions>
	</portlet-resource>
</resource-action-mapping>